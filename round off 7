#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */  
        float a;
        scanf("%f",&a);
         int b=(int)a;
        printf("%d\n",b);
        
        printf("%.1f\n",ceil(a));
        printf("%.1f\n",floor(a));
    return 0;
}
